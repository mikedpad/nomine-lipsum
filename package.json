{
  "name": "nomine-lipsum",
  "version": "4.0.0",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "description": "A random name generator of the most popular names in the USA from 1880 to 2020.",
  "license": "MIT",
  "exports": "./lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [
    "lorem-ipsum",
    "placeholder",
    "name",
    "generator",
    "popular",
    "faker"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mikedpad/nomine-lipsum.git"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "removeTests": "rimraf lib/__tests__",
    "test": "jest src",
    "build:db": "node --loader ts-node/esm src/data/getNameData.ts ",
    "build:lib": "rimraf lib && tsc",
    "build": "npm run build:db && npm run build:lib",
    "prepare": "npm run format && npm run lint && npm run build && npm run test && npm run removeTests"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "adm-zip": "^0.5.9",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "got": "^12.0.2",
    "jest": "^27.5.1",
    "ora": "^6.1.0",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
